%label.spacer-large            
  = t"products"

.row
  - if @delivery_note_products.size > 0
    = render :partial => '/delivery_note_products/list_headers'
    - @delivery_note_products.each do |dnp|
      = render :partial => '/delivery_note_products/list', :locals => { :dnp => dnp }
  - else
    .no-data
      = t"no_data"
      

- unless @disabled
  #hiden-form-cover
  .row.clearfix.last
    #add-new-delivery-note-product-edit.circle-button.large
      = "+"                                                    
        